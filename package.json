{
  "name": "gen",
  "author": "Hares",
  "version": "1.0.3",
  "description": "Cloudflare Worker for PT-Gen Refactor",
  "main": "worker/index.js",
  "scripts": {
    "dev": "cd worker && npx wrangler dev",
    "dev:frontend": "cd frontend && npm run dev",
    "deploy": "cd worker && npx wrangler deploy",
    "deploy:auto": "node deploy.js",
    "deploy:quick": "npm run build:frontend && npm run deploy",
    "build": "node build.js",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run install:all && npm run build:frontend",
    "install:all": "npm install && cd worker && npm install && cd ../frontend && npm install && cd .."
  },
  "keywords": [
    "cloudflare",
    "worker",
    "pt-gen"
  ],
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^4.33.2"
  },
  "dependencies": {
    "cheerio": "^1.1.2"
  }
}
